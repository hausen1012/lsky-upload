{
  "name": "lsky-upload",
  "displayName": "Lsky Upload",
  "description": "lsky 图床 markdown 插件",
  "keywords": [
    "vscode",
    "lsky",
    "markdown"
  ],
  "version": "1.2.5",
  "publisher": "hausen1012",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:lsky-upload",
    "onCommand:lsky-batch-upload"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Lsky Upload",
      "properties": {
        "lsky.baseUrl": {
          "type": "string",
          "default": null,
          "description": "lsky 图床地址"
        },
        "lsky.tokenPath": {
          "type": "string",
          "default": "/api/v1/tokens",
          "description": "获取token路径"
        },
        "lsky.uploadPath": {
          "type": "string",
          "default": "/api/v1/upload",
          "description": "上传接口路径"
        },
        "lsky.email": {
          "type": "string",
          "default": null,
          "description": "图床用户名"
        },
        "lsky.password": {
          "type": "string",
          "default": null,
          "description": "图床密码"
        },
        "lsky.token": {
          "type": "string",
          "default": null,
          "description": "上传接口token, 不填则先获取token, 获取到后保存本地"
        },
        "lsky.tempPath": {
          "type": "string",
          "default": ".tmp",
          "description": "临时图片存储路径"
        },
        "lsky.strategyId": {
          "type": "integer",
          "default": 1,
          "description": "图床中储存策略id"
        },
        "lsky.tinyKeys": {
          "type": "string",
          "default": null,
          "description": "Tinypng调用需要的key列表, 多个key以英文逗号分隔, 调用时随机选取其中一个。（不可以和图床的压缩同时用, 需要在角色组修改图像保存质量为100）"
        },
        "lsky.domainList": {
          "type": "string",
          "default": "",
          "description": "不替换图片地址的域名, 多个域名以英文逗号分隔"
        }
      }
    },
    "commands": [
      {
        "command": "lsky-upload",
        "title": "Lsky Upload"
      },
      {
        "command": "lsky-batch-upload",
        "title": "Lsky Batch Upload"
      }
    ],
    "keybindings": [
      {
        "command": "lsky-upload",
        "key": "ctrl+alt+v",
        "mac": "ctrl+alt+v",
        "when": "editorTextFocus"
      },
      {
        "command": "lsky-batch-upload",
        "key": "ctrl+alt+k",
        "mac": "ctrl+alt+k",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus && resourceLangId == markdown",
          "command": "lsky-upload",
          "group": "navigation@1"
        },
        {
          "when": "editorLangId == 'markdown'",
          "command": "lsky-batch-upload",
          "group": "navigation@12"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.72.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4",
    "vscode": "^1.1.37"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "https": "^1.0.0",
    "moment": "^2.18.1",
    "shelljs": "^0.8.3",
    "tinify": "^1.7.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hausen1012/lsky-upload"
  }
}
